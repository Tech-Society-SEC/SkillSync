{
  "model_name": "xlm-roberta-base",
  "task": "token-classification",
  "num_labels": 3,
  "label2id": {
    "O": 0,
    "B-SKILL": 1,
    "I-SKILL": 2
  },
  "id2label": {
    "0": "O",
    "1": "B-SKILL",
    "2": "I-SKILL"
  },
  "training_args": {
    "num_epochs": 3,
    "batch_size": 16,
    "learning_rate": 2e-05,
    "weight_decay": 0.01,
    "warmup_steps": 500,
    "max_length": 128
  },
  "data_files": {
    "train": "train.json",
    "validation": "val.json"
  }
}